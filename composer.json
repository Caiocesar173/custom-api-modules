{
  "name": "caiocesar173/custom-api-modules-laravel",
  "description": "Custom Api Modules management | Based on modules-laravel",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^6.2",
    "friendsofphp/php-cs-fixer": "^2.16",
    "laravel/framework": "^8.0",
    "spatie/phpunit-snapshot-assertions": "^2.1.0|^4.2",
    "phpstan/phpstan": "^0.12.14"
  },
  "autoload": {
    "psr-4": {
      "Caiocesar173\\Modules\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Caiocesar173\\Modules\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Caiocesar173\\Modules\\LaravelModulesServiceProvider"
      ],
      "aliases": {
        "Module": "Caiocesar173\\Modules\\Facades\\Module"
      }
    }
  },
  "authors": [
    {
      "name": "caiocesar173",
      "email": "caiocesar173caiocesar173@gmail.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true
}